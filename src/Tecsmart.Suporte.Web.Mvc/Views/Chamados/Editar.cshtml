@using Tecsmart.Suporte.Web.Startup
@{
    ViewBag.Title = L("ChamadoEdit");
    ViewBag.CurrentPageName = PageNames.Chamados;
}
@section scripts
{
    <environment names="Development">
        <script src="~/view-resources/Views/Chamados/Editar.js" asp-append-version="true"></script>
    </environment>

    <environment names="Staging,Production">
        <script src="~/view-resources/Views/Chamados/Editar.min.js" asp-append-version="true"></script>
    </environment>
}
<section class="content-header">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-6">
                <h1>@L("ChamadoEdit")</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a asp-controller="Chamados" asp-action="Index">@L("Chamados")</a></li>
                    <li class="breadcrumb-item active">@L("ChamadoEdit")</li>
                </ol>
            </div>
        </div>
    </div>
</section>
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body p-0">
                        <form class="formChamado" method="post">
                            <partial name="_FormChamado" />
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
